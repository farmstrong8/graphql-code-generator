{
    "name": "graphql-codegen-typescript-operation-mocks",
    "version": "0.0.0",
    "description": "a graphql codegen plugin that is useful for generating mocks of your graphql schema and queries",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/farmstrong8/graphql-code-generator.git"
    },
    "homepage": "https://github.com/farmstrong8/graphql-code-generator#readme",
    "bugs": {
        "url": "https://github.com/farmstrong8/graphql-code-generator/issues"
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
        "test": "vitest --run",
        "test:watch": "vitest",
        "test:coverage": "vitest --coverage --run"
    },
    "dependencies": {
        "casual": "^1.6.2",
        "lodash": "^4.17.21",
        "path": "^0.12.7"
    },
    "peerDependencies": {
        "@types/lodash": "^4.0.0",
        "graphql": "^16.0.0",
        "lodash": "^4.0.0"
    },
    "devDependencies": {
        "@graphql-codegen/plugin-helpers": "^5.1.0",
        "@types/lodash": "^4.0.0",
        "@vitest/coverage-v8": "^2.0.0",
        "graphql": "^16.11.0",
        "lodash": "^4.0.0",
        "tsup": "^8.5.0",
        "typescript": "~5.8.3",
        "vitest": "^3.1.4"
    }
}
